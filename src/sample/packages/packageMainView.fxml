<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<?import sample.packages.PackagesTable?>

<fx:root type="VBox" xmlns:fx="http://javafx.com/fxml" xmlns="http://javafx.com/javafx/8.0.121">
   <children>
       <HBox maxHeight="36.0" prefWidth="600.0" spacing="5.0">
           <children>
               <Button fx:id="btnAddPackage" mnemonicParsing="false" text="Add" style="-fx-graphic: url('/icons/small-plus.png')" />
               <Button fx:id="btnDeletePackage" mnemonicParsing="false" text="Delete" style="-fx-graphic: url('/icons/small-minus.png')"/>
           </children>
           <padding>
               <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
           </padding>
       </HBox>
      <PackagesTable fx:id="tabPackages" VBox.vgrow="ALWAYS"/>
   </children>
</fx:root>
